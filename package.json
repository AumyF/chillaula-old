{
  "name": "chillaula-next",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "pages:build": "bunx @cloudflare/next-on-pages@1",
    "pages:deploy": "bun run pages:build && wrangler pages deploy .vercel/output/static",
    "pages:watch": "bunx @cloudflare/next-on-pages@1 --watch",
    "pages:dev": "bunx wrangler pages dev .vercel/output/static --compatibility-date=2023-05-18 --compatibility-flag=nodejs_compat",
    "prepare": "panda codegen",
    "format:write": "prettier --write ."
  },
  "prisma": {
    "schema": "app/_db/schema.prisma"
  },
  "dependencies": {
    "@ariakit/react": "^0.3.5",
    "@types/node": "^20.8.7",
    "@types/react": "^18.2.31",
    "@types/react-dom": "^18.2.14",
    "eslint": "^8.52.0",
    "eslint-config-next": "^14.0.1",
    "kysely": "^0.26.3",
    "kysely-planetscale": "^1.4.0",
    "next": "^14.0.1",
    "prisma": "^5.4.2",
    "prisma-kysely": "^1.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.2.2"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "1",
    "@pandacss/dev": "^0.17.0",
    "eslint-plugin-next-on-pages": "^1.7.1",
    "prettier": "^3.0.3",
    "vercel": "^32.5.0"
  }
}
